{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "initialize git",
            "type": "shell",
            "command": "eval \"$(ssh-agent -s)\" && ssh-add ~/.ssh/gh_key",
            "options": {
                "statusbar": {
                  "label": "$(tools) Initialize git"
                }
              }
        },
        {
            "label": "restart compose",
            "type": "shell",
            "command": "docker compose down && docker compose up -d",
            "options": {
                "statusbar": {
                    "label": "$(refresh) Restart Compose"
                }
            }
        }
    ]
}
